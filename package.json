{
  "name": "telnyx_cypress_cucumber",
  "version": "1.0.0",
  "description": "Project for testing the Telnyx website by cypress+cucumber frameworks",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --config-file config/cypress.config.js",
    "cypress:open": "cypress open --config-file config/cypress.config.js",
    "cypress:run:chrome": "cypress run --config-file config/cypress.config.js --browser chrome",
    "cypress:run:firefox": "cypress run --config-file config/cypress.config.js --browser firefox",
    "cypress:run:edge": "cypress run --config-file config/cypress.config.js --browser edge",
    "cypress:run:viewport:wqhd": "cypress run --config-file config/cypress.wqhd.js",
    "cypress:open:viewport:wqhd": "npx cypress open --config-file config/cypress.wqhd.js",
    "cypress:run:viewport:hd": "npx cypress open --config-file config/cypress.hd.js",
    "cypress:open:viewport:hd": "npx cypress open --config-file config/cypress.hd.js",
    "report:generate": "node ./helper/cucumber-html-report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvansav/telnyx_texting_cypress-cucumber.git"
  },
  "author": "IS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lvansav/telnyx_texting_cypress-cucumber/issues"
  },
  "homepage": "https://github.com/lvansav/telnyx_texting_cypress-cucumber#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@faker-js/faker": "^7.4.0",
    "cypress": "^10.6.0",
    "cypress-real-events": "^1.7.1",
    "esbuild": "^0.14.48",
    "multiple-cucumber-html-reporter": "^1.21.6"
  }
}
